<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>


    <entry key="deleteMenu">
        delete from tbl_menu
        where menu_code = ?
    </entry>

    <entry key="UpdateMenu">
        INSERT INTO TBL_MENU
        (MENU_CODE, MENU_NAME)
        VALUES
        (?,?)
    </entry>
</properties>